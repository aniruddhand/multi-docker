{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "client",
        "image": "aniruddhand/multi-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
      },
      {
          "name": "server",
          "image": "aniruddhand/multi-server",
          "hostname": "api",
          "essential": true,
          "memory": 128
      },
      {
          "name": "worker",
          "image": "aniruddhand/multi-worker",
          "hostname": "worker",
          "essential": true,
          "memory": 128
      }
    ]
}