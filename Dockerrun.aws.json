  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
        "name": "nginx",
        "image": "aniruddhand/multi-nginx",
        "hostname": "nginx",
        "essential": true,
        "memory": 128,
        "portMappings": [{
            "hostPort": 80,
            "containerPort": 80
        }]
    }
  ]
}